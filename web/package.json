{
  "name": "gatsby-template",
  "version": "0.1.0",
  "description": "A Gatsby Template Site",
  "private": true,
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "prod": "gatsby build && gatsby serve",
    "checks": "tsc && npm run lint",
    "lint": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts' --ignore-pattern 'src/assets/*' && stylelint 'src/**/*' --rd --risd",
    "lint:fix": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts'  --ignore-pattern 'src/assets/*' --fix && stylelint 'src/**/*' --rd --risd --fix",
    "func:build": "tsc",
    "start": "gatsby develop"
  },
  "dependencies": {
    "@loadable/component": "^5.13.1",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/image-url": "^0.140.18",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.91",
    "gatsby-image": "^2.4.0",
    "gatsby-plugin-linaria": "^2.0.0",
    "gatsby-plugin-manifest": "^2.4.1",
    "gatsby-plugin-page-creator": "^2.3.7",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.6.33",
    "gatsby-plugin-sitemap": "^2.4.0",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-remark-images": "^3.3.0",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-source-sanity": "^6.0.4",
    "gatsby-transformer-remark": "^2.8.3",
    "gatsby-transformer-sharp": "^2.5.0",
    "isomorphic-fetch": "^3.0.0",
    "google-spreadsheet": "^3.0.13",
    "linaria": "^2.0.0-alpha.2",
    "netlify-lambda": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "sendmail": "1.6.1"
  },
  "devDependencies": {
    "@types/loadable__component": "^5.13.0",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "prettier": "2.0.5",
    "stylelint": "^13.3.3",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "typescript": "^3.7.5"
  }
}
